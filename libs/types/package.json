{
  "name": "@my-english/types",
  "version": "1.0.0",
  "type": "module",
  "description": "Shared TypeScript types for My English Dictionary application",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nx build types",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "keywords": [
    "types",
    "typescript",
    "english",
    "dictionary",
    "learning"
  ],
  "engines": {
    "node": ">=18"
  }
}
